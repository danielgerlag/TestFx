<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>ReSharper.TestFx</id>
    <title>TestFx Runner</title>
    <version>$version$</version>
    <authors>Matthias Koch</authors>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>ReSharper runner for SpecK and MSpec</description>
    <iconUrl>http://matkoch.github.io/TestFx/TestFx.ico</iconUrl>
    <projectUrl>https://github.com/matkoch/TestFx</projectUrl>
    <licenseUrl>https://raw.github.com/matkoch/TestFx/master/LICENSE</licenseUrl>
    <tags>testfx resharper speck mspec</tags>
    <dependencies>
      <dependency id="Wave" version="[7.0]" />
    </dependencies>
  </metadata>
  <files>
    <file src="TestFx.ReSharper\bin\$configuration$\Autofac.*" target="DotFiles" />
    <file src="TestFx.ReSharper\bin\$configuration$\TestFx.*" target="DotFiles" />
    <!--<file src="TestFx.ReSharper\bin\$configuration$\Microsoft.VisualStudio.ExtensionManager.*" target="DotFiles" />-->
    <file src="TestFx.ReSharper\*.DotSettings" target="DotFiles\Extensions\TestFx.ReSharper\settings" />
  </files>
</package>